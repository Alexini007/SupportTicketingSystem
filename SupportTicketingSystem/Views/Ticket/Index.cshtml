@model IEnumerable<SupportTicketingSystem.Data.Ticket>

<h2>Your Recent Tickets</h2>

<table class="table">
    <thead>
        <tr>
            <th>Subject</th>
            <th>Team</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ticket in Model)
        {
            <tr>
                <td>@ticket.Subject</td>
                <td>@ticket.Team</td>
            </tr>
        }
    </tbody>
</table>
